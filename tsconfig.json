{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@c6i/playground": ["src"],
      "@c6i/playground/*": ["src/*", "*"],
      "@js/*": ["docs/notes/js/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "docs/**/*"]
}
