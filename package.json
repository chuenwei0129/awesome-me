{
  "name": "naifu",
  "version": "0.0.1",
  "description": "A react library developed with dumi",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "gh-pages -d docs-dist",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:md": "zhlint \"./docs/**/*.md\" --fix",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev",
    "test:aplus": "promises-aplus-tests ./code/promise/2.promise.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^5.5.1",
    "@antv/x6": "^2.18.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@prisma/client": "5.19.1",
    "@react-spring/web": "^9.7.5",
    "@tabler/icons-react": "^3.21.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@umijs/lint": "^4.0.0",
    "@umijs/plugins": "^4.2.10",
    "@use-gesture/react": "^10.3.1",
    "antd": "^5.21.1",
    "babel-plugin-react-compiler": "19.0.0-beta-63b359f-20241101",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "docx-preview": "^0.3.2",
    "dumi": "^2.3.0",
    "eslint": "^8.23.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-63b359f-20241101",
    "express": "^4.19.2",
    "father": "^4.1.0",
    "framer-motion": "^11.9.0",
    "gh-pages": "^6.2.0",
    "husky": "^8.0.1",
    "immer": "^10.1.1",
    "ioredis": "^5.4.1",
    "jotai": "^2.10.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^13.0.3",
    "lucide-react": "^0.454.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "prisma": "^5.19.1",
    "promises-aplus-tests": "^2.1.2",
    "react": "^18.3.1",
    "react-compiler-runtime": "19.0.0-beta-63b359f-20241101",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-type-animation": "^3.2.0",
    "rehype-mathjax": "^6.0.0",
    "remark-math": "^6.0.0",
    "roughjs": "^4.6.6",
    "scheduler": "^0.23.2",
    "spdy": "^4.0.2",
    "styled-components": "^6.1.11",
    "stylelint": "^14.9.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.5.0",
    "tsd": "^0.31.2",
    "use-context-selector": "^2.0.0",
    "use-immer": "^0.10.0",
    "windups": "^1.2.2",
    "zhlint": "^0.8.1",
    "zustand": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": []
}
