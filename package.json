    "roughjs": "^4.6.6",
    "scheduler": "^0.23.2",
    "spdy": "^4.0.2",
    "sqlite3": "^5.1.6",
    "styled-components": "^6.1.11",
    "stylelint": "^14.9.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.4",
